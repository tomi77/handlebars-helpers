    "templates",
    "coalesce",
    "moment",
    "nl2br"
  ],
  "author": "Tomasz Rup",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tomi77/handlebars-helpers/issues"
  },
  "homepage": "https://github.com/tomi77/handlebars-helpers",
  "dependencies": {
    "handlebars": "~1.3.0",
    "underscore": "~1.7.0",
    "moment": "~2.8.2"
  },
  "devDependencies": {
    "jasmine-node": "^1.14.5",
    "istanbul": "^0.3.2",
    "coveralls": "^2.11.1",
    "coffee-script": "^1.8.0",
    "gulp": "^3.8.8",
    "gulp-uglify": "^1.0.1",
    "gulp-coffee": "^2.2.0",
    "gulp-rename": "^1.2.0",
    "gulp-wrap-umd": "^0.2.1",
    "gulp-concat": "^2.4.0"
